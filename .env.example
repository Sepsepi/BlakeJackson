# Broward Lis Pendens Pipeline Configuration
# =========================================
# Environment variables for Render.com deployment

# Required for Render.com
RENDER=true
BROWARD_HEADLESS=true

# Pipeline configuration
BROWARD_DAYS_BACK=7
BROWARD_BATCH_SIZE=9
BROWARD_MAX_EXECUTION_TIME=7200
BROWARD_MEMORY_CLEANUP_INTERVAL=300

# Optional webhook for status notifications
# BROWARD_WEBHOOK_URL=https://your-webhook-url.com/notify

# Google Sheets Integration (optional)
# GOOGLE_SERVICE_ACCOUNT={"type":"service_account","project_id":"your-project",...}
# GOOGLE_SHEETS_ID=1hcUnRsGfk-lraCrRZYJeeBk1QzszoEEZYPdJn9H21tg
# GOOGLE_SHEETS_SHARE_EMAIL=your-email@gmail.com
# GOOGLE_SHEETS_WORKSHEET_NAME=Broward Lis Pendens
# GOOGLE_SHEETS_APPEND_MODE=true

# Optional skip flags for debugging
# BROWARD_SKIP_SCRAPING=false
# BROWARD_SKIP_PROCESSING=false  
# BROWARD_SKIP_ADDRESS=false
# BROWARD_SKIP_PHONE=false

# Local development only
# BROWARD_OUTPUT_DIR=pipeline_output
# BROWARD_EXCEL_FILE=/path/to/your/excel/file.xlsx
